{
  "extends": [
    "next",
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es2021": true,
    "amd": true,
    "node": true
  },
  "rules": {
    "keyword-spacing": 2,
    "semi": ["error", "never"],
    "quotes": ["error", "double"]
  },
  "plugins": ["react"]
}